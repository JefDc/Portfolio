<!DOCTYPE html>

<title>Extra index</title>

{% block body %}
    <h1>Extra index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>NumberPhone</th>
                <th>Address</th>
                <th>Email</th>
                <th>TextContact</th>
                <th>TextSoftSkill</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for extra in extras %}
            <tr>
                <td>{{ extra.id }}</td>
                <td>{{ extra.numberPhone }}</td>
                <td>{{ extra.address }}</td>
                <td>{{ extra.email }}</td>
                <td>{{ extra.textContact }}</td>
                <td>{{ extra.textSoftSkill }}</td>
                <td>
                    <a href="{{ path('extra_show', {'id': extra.id}) }}">show</a>
                    <a href="{{ path('extra_edit', {'id': extra.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="7">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('extra_new') }}">Create new</a>
{% endblock %}
